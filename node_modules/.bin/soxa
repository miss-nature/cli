#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir/cross-env" ]; then
  exec "$basedir/cross-env" node "$basedir/../voxa-cli-application-1/cli.js" "$@"
else 
  exec cross-env node "$basedir/../voxa-cli-application-1/cli.js" "$@"
fi
